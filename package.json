{
    "name": "ct.nakama",
    "version": "2.3.0",
    "description": "A nakama-js wrapper for ctjs",
    "main": "index.js",
    "scripts": {
        "zip": "npm-build-zip --source=. --destination=./dist/",
        "build": "webpack && npm-build-zip --source=. --destination=./dist/",
