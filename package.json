        "@heroiclabs/nakama-js": "2.3.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "babel-loader": "^8.2.2",
        "eslint": "^7.32.0",
        "jest": "^26.6.3",
        "jest-localstorage-mock": "^2.4.16",
        "npm-build-zip": "^1.0.3",
        "webpack": "^5.50.0",
        "webpack-cli": "^4.7.2"
    }
